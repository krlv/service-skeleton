{
    "name": "krlv/service-skeleton",
    "description": "Slim Service Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Eugene Kirillov",
            "email": "eug.krlv@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "slim/slim": "~4.5",
        "slim/psr7": "~1.1",
        "pimple/pimple": "~3.3",
        "middlewares/trailing-slash": "~2.0",
        "slim/php-view": "~3.0",
        "doctrine/dbal": "~2.10",
        "monolog/monolog": "~2.1",
        "jms/serializer": "~3.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-phpunit": "^0.11",
        "phpstan/phpstan-doctrine": "^0.11",
        "friendsofphp/php-cs-fixer": "^2.16",
        "squizlabs/php_codesniffer": "^3.5",
        "doctrine/coding-standard": "^6.0",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Skeleton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Skeleton\\Test\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin/",
        "preferred-install": "dist"
    }
}
